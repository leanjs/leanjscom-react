{
  "name": "LeanJS",
  "description": "LeanJS website",
  "version": "1.0.0",
  "author": "LeanJS <info@leanjs.com>",
  "dependencies": {
    "babel-core": "6.x.x",
    "babel-loader": "^6.4.1",
    "firebase": "^5.9.0",
    "gatsby": "^1.9.247",
    "gatsby-link": "^1.6.40",
    "gatsby-plugin-google-analytics": "^2.0.18",
    "gatsby-plugin-react-helmet": "^2.0.10",
    "gatsby-plugin-react-next": "^1.0.11",
    "gatsby-plugin-styled-components": "^2.0.11",
    "gatsby-source-filesystem": "^1.5.39",
    "gatsby-transformer-remark": "^1.7.44",
    "react-burger-menu": "^2.5.2",
    "react-cookie": "2.2",
    "react-helmet": "^5.2.0",
    "react-scroll": "^1.7.10",
    "react-storybook-decorator-background": "^1.0.5",
    "react-styled-flexboxgrid": "^2.4.0",
    "react-width": "^0.1.8",
    "styled-components": "^3.3.3"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --trailing-comma --single-quote --no-semi '{src,functions,firebase}/**/*.{js,json,css,md}'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "deploy-storybook": "storybook-to-ghpages",
    "predeploy": "npm run build",
    "build-and-serve": "npm run build && gatsby serve",
    "deploy": "firebase deploy --only hosting"
  },
  "devDependencies": {
    "@storybook/react": "^3.4.8",
    "@storybook/storybook-deployer": "^2.3.0",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "husky": "^1.0.0-rc.13",
    "lint-staged": "^7.2.0",
    "prettier": "^1.12.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  }
